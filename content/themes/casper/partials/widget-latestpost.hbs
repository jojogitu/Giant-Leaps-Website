{{!< default}}
{{#get "posts" limit="5" as |recents|}}
<section class="recent-posts">
  <h5>Latest posts</h5>
  <ol>
    {{#foreach recents}}
    <li><h6><a href="{{url}}">{{title}}</a></h6></li>
    <li><time datetime="{{date format="YYYY-MM-DD"}}">{{date format="DD MMMM YYYY"}}</time></li>
    {{/foreach}}
  </ol>
</section>
{{/get}}
